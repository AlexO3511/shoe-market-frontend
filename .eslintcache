    imageLink: event.target.imageLink.value,\n      userId: event.target.userId.value,\n\n    })\n  }\n\n  getLogin = async (event) => {\n    event.preventDefault()\n    await axios.post(`${backendURL}/auth/login`, {\n      username: event.target.username.value,\n      password: event.target.password.value\n    }).then((response)=>{\n      console.log(response.data.token)\n      let token = response.data.token\n      let decoded = jwt_decode(token);\n      console.log(decoded)\n      this.setState({\n        currentUser: decoded.id\n      })\n      console.log(this.state)\n      \n    })\n  }\n\n  getProfile = async (event) => {\n    event.preventDefault()\n    await axios.get(`${backendURL}/users`)\n  }\n\n  postSignUp = async (event) => {\n    event.preventDefault()\n\n    await axios.post(`${backendURL}/auth/signup`,{\n      name: event.target.name.value, \n      username: event.target.username.value,\n      password: event.target.password.value,\n      email: event.target.email.value\n    })\n  }\n\n  \n  render(){\n    return (\n      <div className=\"App\">\n        <Navbar collapseOnSelect expand=\"lg\" bg=\"dark\" variant=\"dark\">\n        <Nav>\n        <Nav.Link><Link to = '/'>Market Place</Link></Nav.Link>\n        <Nav.Link><Link to = '/login'>Login</Link></Nav.Link>\n        <Nav.Link><Link to = '/signup'>Sign Up</Link></Nav.Link>\n        </Nav>\n        </Navbar>\n        <main>\n          <Switch>\n            <Route exact path='/'\n            component={()=> <Home shoes ={this.state.shoes}/>} />\n\n            < Route path='/login'\n            component={(routerProps)=> <Login {...routerProps} getLogin={this.getLogin}/>} />\n\n            < Route path ='/signup'\n            component={(routerProps)=> <SignUp {...routerProps} postSignUp={this.postSignUp}/>} />\n\n            <Route path ='/users/profile/:id'\n            component={(routerProps)=> <Profile id = {this.state.currentUser} {...routerProps}/>} />\n\n            <Route path ='/users/profile/edit/:id' \n            component={(routerProps)=> <EditProfile {...routerProps}/>} />\n          </Switch>\n        </main>\n   \n      </div>\n    );\n  }\n  }\n\nexport default App;\n","/Users/jo23699/SEI/Unit4/UnitFinal/shoe-market-react-frontend/src/reportWebVitals.js",[],"/Users/jo23699/SEI/Unit4/UnitFinal/shoe-market-react-frontend/src/components/home/home.js",["44","45"],"/Users/jo23699/SEI/Unit4/UnitFinal/shoe-market-react-frontend/src/components/login/login.js",["46"],"import React, {Component} from 'react'; \nimport {Link} from 'react-router-dom';\nimport jwt_decode from \"jwt-decode\";\nimport axios from 'axios';\nimport './login.css'\n\nconst backendURL = \"http://localhost:3000/api\"\nclass Login extends Component{\n    // handleLogin = (event)=>{\n    //     event.preventDefault()\n    //     console.log()\n    //     this.props.getLogin(event)\n    //     this.props.history.push(`/users/profile/${this.props.currentUser}`)\n    // }\n    handleLogin = async (event) => {\n        event.preventDefault()\n        await axios.post(`${backendURL}/auth/login`, {\n          username: event.target.username.value,\n          password: event.target.password.value\n        }).then((response)=>{\n          console.log(response.data.token)\n          let token = response.data.token\n          let decoded = jwt_decode(token);\n          console.log(decoded)\n\n\n          this.props.history.push(`/users/profile/${decoded.id}`)\n        })\n      }\n\n\n    render(){\n        return(\n        <div className=\"login\">\n        {/* <h1>Login</h1> */}\n        <form onSubmit={this.handleLogin} >\n        <h1>Login</h1>\n            <input type =\"text\" name=\"username\" placeholder=\"Enter Username\"/>\n            <input type =\"text\" name=\"password\" placeholder=\"Enter Password\"/>\n            <input type = \"submit\" value=\"Login\"/>\n        </form>\n\n        </div>\n        )\n    }\n}\n\nexport default Login; ","/Users/jo23699/SEI/Unit4/UnitFinal/sh